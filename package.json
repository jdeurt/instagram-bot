{
  "name": "instagram-bot",
  "version": "1.0.0",
  "description": "An instagram like bot.",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "start-nobuild": "ts-node --project tsconfig.json index.ts",
    "build": "npm run build-ts && npm run lint && npm run copy-static-assets",
    "serve": "node dist/index.js",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "keywords": [
    "instagram",
    "bot"
  ],
  "author": "Juan de Urtubey",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/jquery": "^3.3.29",
    "@types/puppeteer": "^1.11.1",
    "@types/shelljs": "^0.8.1",
    "nodemon": "^1.18.9",
    "shelljs": "^0.8.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "enmap": "^4.6.5",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "puppeteer": "^1.11.0",
    "uuid": "^3.3.2"
  }
}
